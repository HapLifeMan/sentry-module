{
  "name": "@nuxtjs/sentry",
  "version": "8.0.0-beta.0",
  "description": "Sentry module for Nuxt.js",
  "repository": "nuxt-community/sentry-module",
  "license": "MIT",
  "contributors": [
    {
      "name": "Diederik van den Burger <diederik@webrelated.nl>"
    },
    {
      "name": "Rafal Chlodnicki (@rchl)"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prepare": "yarn dev:prepare",
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev:fixture": "node ./node_modules/nuxt/bin/nuxt.js -c ./test/fixture/default/nuxt.config.js",
    "analyze": "node ./node_modules/nuxt/bin/nuxt.js build --analyze -c ./test/fixture/default/nuxt.config.js",
    "lint": "eslint --ext .vue,.js,.ts .",
    "lint:fix": "eslint --ext .vue,.js,.ts . --fix",
    "lint:fixture": "eslint --ext .vue,.js --no-ignore 'test/fixture/*/.nuxt/sentry.*'",
    "release": "release-it",
    "test:fixture": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test": "yarn lint && yarn prepack && yarn test:fixture && yarn lint:fixture",
    "coverage": "codecov"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@sentry/integrations": "^7.29.0",
    "@sentry/node": "^7.29.0",
    "@sentry/vue": "^7.29.0",
    "defu": "^6.1.1",
    "lodash.mergewith": "^4.6.2",
    "mlly": "^1.1.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "@release-it/conventional-changelog": "^5.1.1",
    "@sentry/tracing": "^7.29.0",
    "@sentry/webpack-plugin": "^1.20.0",
    "@types/jest": "^29.2.5",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/node": "^16.18.11",
    "codecov": "^3.8.3",
    "consola": "^2.15.3",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.3.1",
    "nuxt": "^3.0.0",
    "playwright-chromium": "^1.29.2",
    "release-it": "^15.6.0",
    "sentry-testkit": "^5.0.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4",
    "vuex": "^3.6.2",
    "webpack": "^4.46.0"
  }
}
