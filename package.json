{
  "name": "@nuxtjs/sentry",
  "version": "6.0.1",
  "description": "Sentry module for Nuxt.js",
  "repository": "nuxt-community/sentry-module",
  "license": "MIT",
  "contributors": [
    {
      "name": "Diederik van den Burger <diederik@webrelated.nl>"
    },
    {
      "name": "Rafal Chlodnicki (@rchl)"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prepare": "yarn dev:prepare",
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev:fixture": "node ./node_modules/nuxt/bin/nuxt.js -c ./test/fixture/lazy/nuxt.config.js",
    "lint": "eslint --ext .vue,.js,.ts src test types",
    "lint:fixture": "eslint --ext .vue,.js --no-ignore 'test/fixture/*/.nuxt/sentry.*'",
    "release": "release-it",
    "test:fixture": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test": "yarn lint && yarn prepack && yarn test:fixture && yarn lint:fixture",
    "coverage": "codecov"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.13",
    "@sentry/browser": "^6.19.7",
    "@sentry/integrations": "^6.19.7",
    "@sentry/node": "^6.19.7",
    "defu": "^6.1.0",
    "lodash.mergewith": "^4.6.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.0",
    "@nuxt/schema": "^3.0.0-rc.13",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "@release-it/conventional-changelog": "^5.1.0",
    "@sentry/minimal": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@sentry/webpack-plugin": "^1.19.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^14.18.31",
    "codecov": "^3.8.3",
    "consola": "^2.15.3",
    "eslint": "^8.26.0",
    "eslint-plugin-jest": "^27.1.4",
    "jest": "^29.2.2",
    "nuxt": "^3.0.0-rc.13",
    "playwright-chromium": "1.26.1",
    "release-it": "^15.4.2",
    "sentry-testkit": "^5.0.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vuex": "^3.6.2",
    "webpack": "^4.46.0"
  }
}
